{
  "root": "apps/sso",
  "sourceRoot": "apps/sso/src",
  "projectType": "application",
  "implicitDependencies": [
    "config-service",
    "shared-utils-server"
  ],
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/sso",
        "main": "apps/sso/src/server.ts",
        "tsConfig": "apps/sso/tsconfig.app.json",
        "assets": ["apps/sso/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/sso/src/environments/environment.ts",
              "with": "apps/sso/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "sso:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/sso/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/sso"],
      "options": {
        "jestConfig": "apps/sso/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "SSO"
  ]
}
