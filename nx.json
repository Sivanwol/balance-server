{
  "npmScope": "balancer",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "ZTI1YjNmMjYtY2MxZi00OGZkLWFiMTMtZjhjYmYyMTNjY2U0fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "defaultProject": "config",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styl",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styl"
      },
      "library": {
        "style": "styl",
        "linter": "eslint"
      }
    }
  }
}
