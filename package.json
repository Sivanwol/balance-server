{
  "name": "balance_buget_server",
  "version": "1.0.1",
  "license": "private",
  "scripts": {
    "clear:unused:deps": "pnpm nx generate @trumbitta/nx-plugin-unused-deps:check --no-interactive",
    "generate:server:app": "pnpm nx g @nrwl/express:app ",
    "generate:server:lib": "pnpm nx g @nrwl/node:library",
    "generate:secret": "node -e \"console.log(crypto.randomBytes(32).toString('hex'))\"",
    "generate:secret:service": "node ./scripts/create_api_key.js",
    "start": "nx serve",
    "start:service:config": "pnpm prisma:generate && pnpm run build:service:config && node_modules/.bin/nx serve config-service",
    "start:service:api": "pnpm prisma:generate && pnpm run build:service:api && node_modules/.bin/nx serve api",
    "start:service:sso": "pnpm prisma:generate && pnpm run build:service:sso && node_modules/.bin/nx serve sso",
    "start:all:debug": "concurrently \"pnpm run start:service:config\" \"pnpm run start:service:api\" \"pnpm run start:service:sso\"",
    "start:all": "concurrently \"nx run api:serve\"  \"ng run sso:serve\"",
    "build": "nx build",
    "build:service:config": "node_modules/.bin/nx build config-service",
    "build:service:api": "node_modules/.bin/nx build api",
    "build:service:sso": "node_modules/.bin/nx build sso",
    "test": "nx test",
    "commit": "cz",
    "prepare": "husky install",
    "lint": "pnpm lint:apps && pnpm lint:libs",
    "lint:fix": "pnpm lint:apps:fix && pnpm lint:libs:fix",
    "lint:apps": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts apps/**/src",
    "lint:apps:fix": "pnpm run lint -- --fix",
    "lint:libs": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts libs/**/src",
    "lint:libs:fix": "pnpm run lint -- --fix",
    "version": "conventional-changelog -i CHANGELOG.md -s",
    "prisma:init": "npx prisma init",
    "prisma:migrate": "npx prisma migrate dev --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:seed": "npx prisma db seed",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:reset": "npx prisma migrate reset"
  },
