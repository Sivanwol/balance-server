{
  "name": "balance_buget_server",
  "version": "1.0.1",
  "license": "private",
  "scripts": {
    "clear:unused:deps": "npm nx generate @trumbitta/nx-plugin-unused-deps:check --no-interactive",
    "generate:server:app": "npm nx g @nrwl/node:application ",
    "generate:server:lib": "npm nx g @nrwl/node:library",
    "generate:secret": "node -e \"console.log(crypto.randomBytes(64).toString('hex'))\"",
    "start": "nx serve",
    "start:service:config": "npm run build:service:config && node_modules/.bin/nx serve config-service",
    "start:service:api": "npm run build:service:api && node_modules/.bin/nx serve api-gate-way",
    "start:service:sso": "npm run build:service:sso && node_modules/.bin/nx serve sso",
    "start:all:debug": "concurrently \"npm run start:service:config\" \"npm run start:service:api\" \"npm run start:service:sso\"" ,
    "start:all": "concurrently \"nx run api-gate-way:serve\" \"nx run config-service:serve\"  \"ng run sso:serve\"",
    "build": "nx build",
    "build:service:config": "node_modules/.bin/nx build config-service",
    "build:service:api": "node_modules/.bin/nx build api-gate-way",
    "build:service:sso": "node_modules/.bin/nx build sso",
    "test": "nx test",
    "commit": "cz",
    "prepare": "husky install",
    "lint": "npm lint:apps && npm lint:libs",
    "lint:fix": "npm lint:apps:fix && npm lint:libs:fix",
    "lint:apps": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts apps/**/src",
    "lint:apps:fix": "npm run lint -- --fix",
    "lint:libs": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts libs/**/src",
    "lint:libs:fix": "npm run lint -- --fix",
    "version": "conventional-changelog -i CHANGELOG.md -s",
    "prisma:init": "npx prisma init",
    "prisma:migrate": "npx prisma migrate dev --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:seed": "npx prisma db seed",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:reset": "npx prisma migrate reset"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
