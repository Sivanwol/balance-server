  "scripts": {
    "clear:unused:deps": "pnpm run nx generate @trumbitta/nx-plugin-unused-deps:check --no-interactive",
    "generate:server:app": "pnpm run nx g @nrwl/express:app ",
    "generate:server:lib": "pnpm run nx g @nrwl/node:library",
    "generate:secret": "node -e \"console.log(crypto.randomBytes(32).toString('hex'))\"",
    "generate:secret:service": "node ./scripts/create_api_key.js",
    "start": "nx serve",
    "start:service:operations": "pnpm prisma:generate && pnpm exec nx run operations-service:serve",
    "start:client:backoffice:dev": "pnpm run build:client:backoffice && pnpm run nx run backoffice-ui:serve:development",
    "start:client:backoffice": "pnpm run build:client:backoffice && pnpm run nx run backoffice-ui:serve",
    "storybook:client:backoffice": "pnpm run build:client:backoffice && nx run backoffice-ui:storybook",
    "start:all": "concurrently \"pnpm run nx run apoperations-service:serve\" ",
    "build": "nx build",
    "build:service:api": "pnpm run nx build operations-service",
    "build:client:backoffice": "pnpm run nx run backoffice-ui:build",
    "test": "pnpm run nx test",
    "commit": "cz",
    "prepare": "husky install",
    "lint": "pnpm lint:apps && pnpm lint:libs",
    "lint:fix": "pnpm lint:apps:fix && pnpm lint:libs:fix",
    "lint:apps": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts apps/**/src",
    "lint:apps:fix": "pnpm run lint -- --fix",
    "lint:libs": "eslint -c .eslintrc --ignore-path .gitignore --ext .ts libs/**/src",
    "lint:libs:fix": "pnpm run lint -- --fix",
    "version": "conventional-changelog -i CHANGELOG.md -s",
    "prisma:init": "npx prisma init",
    "prisma:migrate": "npx prisma migrate dev --preview-feature --create-only",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma format",
    "prisma:seed": "npx prisma db seed",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "prisma:reset": "npx prisma migrate reset"
  },
